<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kartik.lenovo_pc.cinfokorder">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cinfokicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminLogin"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            />
        <activity android:name=".AdminMainPage"
            android:screenOrientation="portrait"
            />
        <activity android:name=".CustomerMainPage"
            android:screenOrientation="portrait"
            />
        <activity android:name=".Cart"
            android:screenOrientation="portrait"
            />
        <activity android:name=".MyOrders"
            android:screenOrientation="portrait"
            />
        <activity android:name=".AddDish"
            android:screenOrientation="portrait"
            />
        <activity android:name=".DishData"
            android:screenOrientation="portrait"
            />
    </application>

</manifest>